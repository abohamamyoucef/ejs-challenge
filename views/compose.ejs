<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="title">Title</label>
        <input class="form-control" type="text"name="postTitle" id="title">
        <label for="body">Post</label>
        <textarea class="form-control" type="text"name="postBody" id="body" rows="5" cols="30"></textarea> 
    </div>
    <button class="btn btn-primary" type="submit" name="btpublish">Publish </button>

</form>

<%- include('partials/footer'); -%>